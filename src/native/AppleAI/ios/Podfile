# Podfile for Apple AI Module
# Optimized for iOS 18+ with latest Apple Intelligence APIs
# This is automatically included by Expo when building

platform :ios, '18.0'

target 'AppleAI' do
  use_frameworks!
  use_modular_headers!
  
  # Core ML and Natural Language are included in iOS SDK
  # Enhanced for latest iOS versions with Neural Engine support
  # No additional pods needed - using native frameworks:
  # - CoreML (with Neural Engine optimization)
  # - NaturalLanguage (enhanced NLP)
  # - AppIntents (Apple Intelligence integration)
  # - MLCompute (advanced ML operations)
end

